{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileStorage": {
    "BasePath": "./uploads"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=gso_library;Username=gso_app;Password=gso_app_password"
  },
  "Jwt": {
    "SecretKey": "your-super-secret-key-min-32-characters-long-change-this!",
    "Issuer": "GSOLibrary",
    "Audience": "GSOLibraryUsers"
  },
  "SeedUsersFile": "seed_users.json",
  "FileUpload": {
    "AllowedExtensions": [".pdf", ".xml", ".mxl", ".mid", ".midi", ".mp3", ".wav", ".flac", ".ogg", ".mscz", ".dorico", ".sib"],
    "MaxFileSizeBytes": 52428800
  }
}
